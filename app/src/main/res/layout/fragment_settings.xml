    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.SettingsFragment">



    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@drawable/background_cadastro_gradiente"
        android:layout_height="match_parent">

        <include
            layout="@layout/toolbar_pincipal" />

        <Button
